\begin{document}

\textbf{Linear Equations}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
3x+7-(5-2x)=6x-2  
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
7-3(2t-5)+5(9-3t)=5(1-7t)-2(4-3t)
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
(3x-5)^{2}-(2x+3)^{2}=5(x-2)(x+2)-2(14x+3)
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
(3y-2)(y-4)-(2y-3)^{2}+y(y-3)=7(y-1)-6(y+2)
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
99-10t(1-2t)=8t(t-1)^{2}-(2t-3)^{3}
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
\frac{3+5x}{8}-\frac{2(3x-1)}{4}=\frac{1+x}{2}+10
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
x+\frac{x+3}{2}-\frac{x}{4}-1=\frac{x+4}{8}
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
\frac{s}{2}+\frac{s}{3}-\frac{s}{4}=\frac{7s+5}{12}
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
3x+7-(5-2x)=6x-2  
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
3x+7-(5-2x)=6x-2  
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
3x+7-(5-2x)=6x-2  
\end{equation}

{\aufgabe} Solve the following equation:
\begin{equation}
  \label{eq:iexaigee}
3x+7-(5-2x)=6x-2  
\end{equation}

